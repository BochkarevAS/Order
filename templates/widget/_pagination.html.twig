            <div class="pag-inner">
                {% if startPage > 1 %}
                    <a href="{{ path(route, query|merge({(pageParameterName): 1})) }}" class="pag-inner__link">1</a>
                    {% if startPage == 3 %}
                        <a href="{{ path(route, query|merge({(pageParameterName): 2})) }}" class="pag-inner__link">2</a>
                    {% elseif startPage != 2 %}
                        <a href="#" class="pag-inner__link">...</a>
                    {% endif %}
                {% endif %}

                {% for page in pagesInRange %}
                    {% if page != current %}
                        <a href="{{ path(route, query|merge({(pageParameterName): page})) }}" class="pag-inner__link">{{ page }}</a>
                    {% else %}
                        <a href="#" class="pag-inner__link active">{{ page }}</a>
                    {% endif %}
                {% endfor %}

                {% if pageCount > endPage %}
                    {% if pageCount > (endPage + 1) %}
                        {% if pageCount > (endPage + 2) %}
                            <a href="#" class="pag-inner__link">...</a>
                        {% else %}
                            <a href="{{ path(route, query|merge({(pageParameterName): (pageCount - 1)})) }}" class="pag-inner__link">{{ pageCount -1 }}</a>
                        {% endif %}
                    {% endif %}
                    <a href="{{ path(route, query|merge({(pageParameterName): pageCount})) }}" class="pag-inner__link">{{ pageCount }}</a>
                {% endif %}
            </div>

